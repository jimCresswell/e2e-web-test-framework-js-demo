    "@wdio/local-runner": "^5.13.1",
    "@wdio/selenium-standalone-service": "^5.16.5",
    "@wdio/spec-reporter": "^5.12.1",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "open-cli": "^5.0.0"
  },
  "devDependencies": {
